[alias]
	ci = commit
	st = status
	br = branch
	co = checkout
	df = diff
	lg = log --graph
	lo = log --pretty=oneline
	pu = !"git fetch origin; git fetch upstream; git merge upstream/master"
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan