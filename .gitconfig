[user]
	email = i@foohack.com
	name = isaacs
[alias]
	ci = commit
	st = status
	br = branch
	co = checkout
	df = diff
	lg = log -p
	pu = !"git fetch origin -v; git fetch upstream -v; git merge upstream/master"
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan